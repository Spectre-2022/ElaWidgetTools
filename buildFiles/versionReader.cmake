#@NOTE:从version.txt读取版本号
file(STRINGS "./../version.txt" PROINFO_STR)
string(REPLACE ":" ";" PROINFO_LST ${PROINFO_STR})
list(GET PROINFO_LST 0 LIB_NAME)
list(GET PROINFO_LST 1 VERSION)
string(REPLACE "." ";" VERSION_LST ${VERSION})
list(GET VERSION_LST 0 VERSION_FIRST_NUMBER)
list(GET VERSION_LST 1 VERSION_SECOND_NUMBER)
list(GET VERSION_LST 2 VERSION_THIRD_NUMBER)
list(GET VERSION_LST 3 VERSION_FOURTH_NUMBER)
list(GET VERSION_LST 4 VERSION_HOTFIX_NUMBER)
list(JOIN VERSION_LST "." LIB_VER)
