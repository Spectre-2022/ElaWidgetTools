1 VERSIONINFO
 FILEVERSION ${VERSION_FIRST_NUMBER}, ${VERSION_SECOND_NUMBER}, ${VERSION_THIRD_NUMBER}, ${VERSION_FOURTH_NUMBER}
 PRODUCTVERSION ${VERSION_FIRST_NUMBER}, ${VERSION_SECOND_NUMBER}, ${VERSION_THIRD_NUMBER}, ${VERSION_FOURTH_NUMBER}
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription", "${PROJECT_NAME}"
            VALUE "FileVersion", "${VERSION_FIRST_NUMBER}, ${VERSION_SECOND_NUMBER}, ${VERSION_THIRD_NUMBER}"
            VALUE "InternalName", "${PROJECT_NAME}"
            VALUE "LegalCopyright", "Copyright (C) 2023 The Lksense Company Ltd"
            VALUE "OriginalFilename", ""
            VALUE "ProductName", "${PROJECT_NAME}"
            VALUE "ProductVersion", "${VERSION_FIRST_NUMBER}.${VERSION_SECOND_NUMBER}.${VERSION_THIRD_NUMBER}.${VERSION_FOURTH_NUMBER}.${VERSION_HOTFIX_NUMBER}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
